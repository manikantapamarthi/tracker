<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Role</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td><%= link_to "Edit" %> <%= link_to "Delete" %></td>
    </tr>
    <% end %>
  </tbody>
</table>